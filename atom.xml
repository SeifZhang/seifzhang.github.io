    public int[] twoSum(int[] nums, int target) {
          for (int i = 0; i &amp;lt; nums.length; i++) {
            for (int j = i + 1; j &amp;lt; nums.length; j++) {
                if ((nums[i] + nums[j]) == target) {
                    return new int[] { i, j };
                }
            }
        }
    return null;
    }
}
&lt;/code&gt;&lt;/pre&gt;</content>
    
